                                    @{ var currseat = ""; var oldseat = ""; }
                                    @foreach (var item in Model.tbl_Q7)
                                    {
                                        currseat = item.Seat_No;
                                        for (int i = 1; i <= 3; i++)
                                        {
                                            if (oldseat != currseat || oldseat == "")
                                            {
                                                <tr>
                                                    <td>
                                                        @i
                                                    </td>
                                                    <td>
                                                        @item.Index_No
                                                    </td>
                                                    <td>
                                                        @item.Seat_No
                                                    </td>
                                                    <td>
                                                        @item.Paper_No
                                                    </td>
                                                    @if (i == 1)
                                                    {
                                                        <td>
                                                            Response
                                                        </td>
                                                        foreach (var it in Model.tbl_Q7)
                                                        {
                                                            if (it.Seat_No == currseat)
                                                            {
                                                                <td>
                                                                    @it.Answer
                                                                </td>
                                                            }
                                                        }
                                                    }

                                                    @if (i == 2)
                                                    {
                                                        <td>
                                                            Model Answer
                                                        </td>
                                                        foreach (var ans in Model.tbl_Model_Answer)
                                                        {
                                                            foreach (var it in Model.tbl_Q7)
                                                            {
                                                                if (it.Question_ID == ans.ID && it.Seat_No == currseat)
                                                                {
                                                                    <td>
                                                                        @ans.Answer1
                                                                    </td>
                                                                }
                                                            }
                                                        }

                                                    }
                                                    @if (i == 3)
                                                    {
                                                        <td>
                                                            Marks
                                                        </td>

                                                        foreach (var ans in Model.tbl_Model_Answer)
                                                        {
                                                            foreach (var it in Model.tbl_Q7)
                                                            {
                                                                if (it.Question_ID == ans.ID && it.Seat_No == currseat)
                                                                {
                                                                    <td>
                                                                        @ans.Marks
                                                                    </td>
                                                                }
                                                            }
                                                        }

                                                    }

                                                </tr>
                                            }
                                        }
                                        oldseat = currseat;
                                        currseat = "";

                                    }

                                </tbody>
                            </table>
                            <br>
                        </td>
                    </tr>

                </table>
            }
        </div>
        
    }
</div>

